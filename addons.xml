<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.pelisalacarta"
    name="pelisalacarta"
    version="4.2.0~beta2"
    provider-name="tvalacarta">
  <requires>
    <import addon="xbmc.python" version="2.2.0"/>
    <import addon="script.module.libtorrent" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Películas y series a la carta</summary>
    <description lang="es">Películas, series, documentales, anime, ... todo lo que puedas encontrar en Internet ahora desde tu media center. Incluye canales en Español, Italiano e Inglés.</description>
    <summary lang="en">Movies and TV Shows on demand</summary>
    <description lang="en">Movies, Series, Documentaries, Anime, ... everything you can find on Internet now on your media center. Includes channels in Spanish, Italian and English.</description>
    <platform>all</platform>
    <license>GNU GPL v3</license>
    <forum>http://www.mimediacenter.info/foro</forum>
    <website>http://blog.tvalacarta.info/plugin-xbmc/pelisalacarta</website>
    <email>tvalacarta@gmail.com</email>
    <source>https://github.com/tvalacarta/pelisalacarta</source>
  </extension>
  <extension point="xbmc.service" library="library_service.py" start="login|startup">
  </extension>
</addon>

<addon
  id="plugin.video.viki"
  version="1.1.44"
  name="Viki"
  provider-name="Dknight">
  <requires>
		<import addon="script.module.beautifulsoup" version="3.0.8"/>
		<import addon="script.module.simplejson" />
		<import addon="script.module.urlresolver"  version="0.0.1"/>
		<import addon="script.module.t0mm0.common" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Viki.com</summary>
    <description>Watch Viki.com Videos</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.josuecocr" name="josuecocr official Repo" version="1.1.1" provider-name="josuecocr">
	<extension point="xbmc.addon.repository" name="josuecocr official Repo">
		<info compressed="false">http://raw.githubusercontent.com/josuecocr/josuerepo/master/addons.xml</info>
		<checksum>http://raw.githubusercontent.com/josuecocr/josuerepo/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.githubusercontent.com/josuecocr/josuerepo/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>josuecocr official Repo</summary>
   		<description>Instala los Addons de josuecocr</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
