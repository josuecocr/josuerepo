<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.adryanlist"
       name="AdryanList"
       version="2.6.7"
       provider-name="Doc Shadrach">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.liveresolver" version="0.1.24" optional="true"/>
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" optional="true" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
     <import addon="script.module.livestreamer" optional="true"/>
     <import addon="script.module.pyamf" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>based on livestreamspro 2.6.7 by Shani</summary>
    <description>Lista de canales de Adryan.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.pelisalacarta"
    name="pelisalacarta"
    version="4.2.0~beta3"
    provider-name="tvalacarta">
  <requires>
    <import addon="xbmc.python" version="2.2.0"/>
    <import addon="script.module.libtorrent" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Películas y series a la carta</summary>
    <description lang="es">Películas, series, documentales, anime, ... todo lo que puedas encontrar en Internet ahora desde tu media center. Incluye canales en Español, Italiano e Inglés.</description>
    <summary lang="en">Movies and TV Shows on demand</summary>
    <description lang="en">Movies, Series, Documentaries, Anime, ... everything you can find on Internet now on your media center. Includes channels in Spanish, Italian and English.</description>
    <platform>all</platform>
    <license>GNU GPL v3</license>
    <forum>http://www.mimediacenter.info/foro</forum>
    <website>http://blog.tvalacarta.info/plugin-xbmc/pelisalacarta</website>
    <email>tvalacarta@gmail.com</email>
    <source>https://github.com/tvalacarta/pelisalacarta</source>
  </extension>
  <extension point="xbmc.service" library="library_service.py" start="login|startup">
  </extension>
</addon>

<addon id="repository.dk-xbmc-repaddon" name="dk-xbmc-repaddon Add-on Repository" version="1.0.5" provider-name="dk-xbmc-repaddon">
	<extension point="xbmc.addon.repository" name="dk-xbmc-repaddon Add-on Repository">
		<info compressed="false">http://raw.githubusercontent.com/dknlght/dkodi/master/addons.xml</info>
		<checksum>http://raw.githubusercontent.com/dknlght/dkodi/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.githubusercontent.com/dknlght/dkodi/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Add-ons from dk Add-on Repository</summary>
		<description>Video plug-ins for XBMC</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="repository.josuecocr" name="josuecocr official Repo" version="1.1.1" provider-name="josuecocr">
	<extension point="xbmc.addon.repository" name="josuecocr official Repo">
		<info compressed="false">http://raw.githubusercontent.com/josuecocr/josuerepo/master/addons.xml</info>
		<checksum>http://raw.githubusercontent.com/josuecocr/josuerepo/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.githubusercontent.com/josuecocr/josuerepo/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>josuecocr official Repo</summary>
   		<description>Instala los Addons de josuecocr</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
